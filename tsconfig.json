{
	"compilerOptions": {
		"module": "commonjs",
		"checkJs": true,
		"allowJs": true,
		"declaration": true,
		"removeComments": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"target": "es2017",
		"sourceMap": true,
		"outDir": "./dist",
		"baseUrl": "./",
		"incremental": true,
		"paths": {
			// "@application/*": [
			// 	"src/*"
			// ],
			"@common/*": [
				"src/common/*"
			],
			"@model/*": [
				"src/model/*"
			],
			"@enum/*": [
				"src/enum/*"
			],
			"@component/*": [
				"src/component/*"
			]
		}
	}
}